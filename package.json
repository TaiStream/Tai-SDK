{
    "name": "@tai-network/sdk",
    "version": "0.1.0",
    "description": "Developer SDK for Tai Network - Decentralized Video Infrastructure on Walrus",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./types": {
            "types": "./dist/types.d.ts",
            "require": "./dist/types.js",
            "default": "./dist/types.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "sideEffects": false,
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "npx tsx src/__tests__/walrus.test.ts",
        "test:unit": "npx tsx src/__tests__/sdk.test.ts",
        "test:upload": "npx tsx src/__tests__/walrus.test.ts"
    },
    "keywords": [
        "tai",
        "walrus",
        "sui",
        "video",
        "decentralized",
        "streaming",
        "upload",
        "storage",
        "web3"
    ],
    "author": "Tai Network",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Legatia/Tai",
        "directory": "Tai-SDK"
    },
    "homepage": "https://github.com/Legatia/Tai/tree/main/Tai-SDK",
    "bugs": {
        "url": "https://github.com/Legatia/Tai/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "devDependencies": {
        "typescript": "^5.3.0",
        "tsx": "^4.7.0"
    }
}
